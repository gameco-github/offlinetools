<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mycompany.myteam.myapp" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <icon src="www/img/logo.png" />
    <name>GamecoTools</name>
    <description>
        Aplicacion Desarrollada por Eligio Arreola
    </description>
    <author email="eligo_arreola@hotmail.com">
        Desarollo Eligio Arreola
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
